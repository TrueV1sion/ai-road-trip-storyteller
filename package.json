{
  "name": "ai-road-trip-storyteller",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "dev": "npx expo start --web",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "build:web": "npx expo export --platform web",
    "build:netlify": "npx expo export --platform web && cp _redirects dist/",
    "postbuild": "cp public/* dist/ 2>/dev/null || true",
    "serve:web": "npx serve dist",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "npx expo install --fix"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "~52.0.30",
    "expo-application": "~6.0.2",
    "expo-camera": "~16.0.8",
    "expo-constants": "~17.0.3",
    "expo-device": "~6.0.2",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-location": "~18.0.3",
    "expo-router": "~4.0.17",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.12",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.4",
    "expo-web-browser": "~14.0.1",
    "lucide-react-native": "^0.294.0",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.14.0",
    "react-native-svg": "15.9.0",
    "react-native-web": "~0.19.12",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@expo/webpack-config": "^19.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.8",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^8.55.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.1",
    "typescript": "^5.3.0"
  },
  "private": true,
  "license": "MIT"
}
